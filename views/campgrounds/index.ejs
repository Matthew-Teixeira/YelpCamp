    <% layout('layout/boilerplate') %>

    <div id="cluster-map"></div>

       <h1>All Capmgrounds</h1>
       <div>
       </div>
       <ul>
           <%for(let camp of campgrounds){%>
               <div class="card mb-3">
                   <div class="row">
                    <div class="col-md-4">
                        <% if(camp.images.length) { %>
                            <img class="img-fluid" alt="" src="<%=camp.images[0].url%>">
                        <% } else { %>
                                <img src='https://res.cloudinary.com/apollostudio/image/upload/v1642352127/YelpCamp/rxueudfznteslfxuz2yc.png'>
                            <%} %>
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title"><%= camp.title %></h5>
                            <p class="card-text"><%= camp.description %></p>
                            <p class="card-text">
                                <small class="text-muted"><%= camp.location %></small>
                            </p>
                            <a href="campgrounds/<%=camp._id%>" class="btn btn-primary">View <%= camp.title %></a>
                        </div>
                    </div>
                   </div>
               </div>
           <%}%>
       </ul>

       <script>
        const mapToken = '<%- process.env.MAPBOX_TOKEN %>';
        const camp = {features: <%- JSON.stringify(campgrounds) %>}
   </script>

       <script src="/js/clusterMap.js"></script>